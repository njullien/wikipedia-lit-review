@InProceedings{NonneckePreeceAndrews04,
  Title                    = {What lurkers and posters think of each other},
  Author                   = {Blair Nonnecke and Jenny Preece and Dorine Andrews},
  Booktitle                = {Proceedings of the Thirty seventh Hawai{\textquoteright}i International Conference on System Science (HICSS-37)},
  Year                     = {2004},
  Publisher                = {John Wiley \& Sons},

  Abstract                 = {This study reports the results of an online survey which generated 1188 responses from 375 online MSN communities. The survey examined the behavior and attitudes of participants who post (i.e., posters) and those who read but do not post (i.e., lurkers). The results of the analysis indicate that posters and lurkers go online for similar reasons. While lurkers did not publicly ask questions, they wanted answers to questions (62.1% vs. 70.3% for posters). However, lurkers were less enthusiastic about the benefits of community membership, with 41.8% indicating they received less than the expected benefit. In contrast, 36.6% of posters perceived a greater than expected benefit.
}
}

@InCollection{Nov09,
