@Article{FerraryDibiaggio03,
  Title                    = {Communaut{\'{e}}s de pratique et r{\'{e}}seaux sociaux dans la dynamique de fonctionnement des clusters de hautes technologies.},
  Author                   = {Michel Ferrary and Ludovic Dibiaggio},
