@InProceedings{LamKarimRiedl10,
  Title                    = {The effects of group composition on decision quality in a social production community},
  Author                   = {Lam,S. K. and Karim,J. and Riedl,J.},
  Booktitle                = {Proceedings of the 16th ACM International Conference on Supporting Group Work, GROUP'10},
  Year                     = {2010},
  Editor                   = { },
  Pages                    = {55-64},

  Abstract                 = {Online social production communities allow efficient construction of valuable and high-quality information sources. To be successful, community members must be effective at collaboration, including makink collective decisions in the presence of disagreement. We examined over 100,000 decisions made by small working groups in Wikipedia, and analyzed how decision quality in these online groups is influenced by four group composition factors: the size of the group, how members were invited to the group, the prior experience of group members, and apparent bias shown by the group administrator. Our findings lead us to recommendations for designers of social production communities.
}
}

@Article{LamRiedl11,
  Title                    = {The Past, Present, and Future of Wikipedia},
  Author                   = {Lam, Shyong K. and Riedl, John},
  Journal                  = {Computer},
  Year                     = {2011},

  Month                    = mar,
  Number                   = {3},
  Pages                    = {87-90},
  Volume                   = {44},

  Abstract                 = {The writers discuss the growth of Wikipedia. The first community-developed growth model correctly predicted exponential growth in the first few years, but the growth rate peaked in 2006. A revised community-developed model assumes that there is a maximum amount of encyclopedic content and predicts an S-shaped growth curve. Researchers at the Palo Alto Research Center in California claim that their ecological model also explains the slowdown in growth that Wikipedia has been experiencing in recent years. They equate limited environmental resources with the finite amount of encyclopedic knowledge and compare conflict among Wikipedia's editing community to resource competition. Time spent by editors on conflicts and coordination activity is increasing at the expense of directly productive work, a phenomenon that could result in gridlock or worthwhile improvements in quality.}
}

@InProceedings{Lametal11,
  Title                    = {{WP:Clubhouse? An Exploration of Wikipedia's Gender Imbalance}},
  Author                   = {Lam, Shyong K. and Uduwage, Anuradha and Dong, Zhenhua and Sen, Shilad and Musicant, David R. and Terveen, Loren and Riedl, John},
  Booktitle                = {Proceedings of the 7th International Symposium on Wikis and Open Collaboration},
  Year                     = {2011},

  Address                  = {New York, NY, USA},
  Month                    = oct,
  Publisher                = {ACM},
  Series                   = {WikiSym '11},

  Abstract                 = {Wikipedia has rapidly become an invaluable destination for millions of information-seeking users. However, media reports suggest an important challenge: only a small fraction of Wikipedia’s legion of volunteer editors are female. In the current work, we present a scientific exploration of the gender imbalance in the English Wikipedia’s population of editors. We look at the nature of the imbalance itself, its effects on the quality of the encyclopedia, and several conflict-related factors that may be contributing to the gender gap. Our findings confirm the presence of a large gender gap among editors and a corresponding gender-oriented disparity in the content of Wikipedia’s articles. Further, we find evidence hinting at a culture that may be resistant to female participation.},
  Keywords                 = {Wikipedia, collaboration, gender gap, content coverage},
  Location                 = {Mountain View, California}
}

