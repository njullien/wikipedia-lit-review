@Article{Weinschenk11,
  Title                    = {{The Provision of Perks, Moral Hazard and Limited Liability}},
  Author                   = {Philipp Weinschenk},
  Journal                  = {{Max Plan Institute Economics Workshop}},
  Year                     = {2011},

  Abstract                 = {We explore the provision of perks in a principal-agent model with moral hazard. Perks are related to agent’s work and therefore allow the principal to cut back monetary incentives. Our main result is that even though perks are contractible, their provision may be inefficient. The principal tends to overinvest [underinvest] in perks which are closely [little] related to work. Further results are that (i) the provision of perks may actually hurt the agent, even though perks per se are enjoyable for him, (ii) the principal may prefer an organizational structure with high costs of monitoring, and (iii) when there are several types of perks, the principal may choose other perks than the social planner would choose},
  Url                      = {http://www.coll.mpg.de/sites/www.coll.mpg.de/files/workshop/perks12.pdf}
