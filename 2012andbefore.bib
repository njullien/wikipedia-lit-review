  Author                   = {Morris, Meredith Ringel and Teevan, Jaime and Panovich, Katrina},
  Booktitle                = {Proceedings of the 28th international conference on Human factors in computing systems},
  Year                     = {2010},

  Address                  = {New York, NY, USA},
  Pages                    = {1739--1748},
