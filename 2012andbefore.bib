@InProceedings{Burioletal06,
  Title                    = {{Temporal Evolution of the Wikigraph}},
  Author                   = {Buriol, Luciana and Castillo, Carlos and Donato, Debora and Leonardi, Stefano and Millozzi, Stefano},
