@Article{Maillartetal08,
  Title                    = {{Empirical Tests of Zipf's Law Mechanism in Open Source Linux Distribution}},
  Author                   = {Maillart, T. and Sornette, D. and Spaeth, S. and von Krogh, G.},
  Journal                  = {Physical Review Letters},
  Year                     = {2008},

  Month                    = {Nov},
  Pages                    = {218701},
  Volume                   = {101},

  Abstract                 = {Zipf’s power law is a ubiquitous empirical regularity found in many systems, thought to result from proportional growth. Here, we establish empirically the usually assumed ingredients of stochastic growth models that have been previously conjectured to be at the origin of Zipf’s law. We use exceptionally detailed data on the evolution of open source software projects in Linux distributions, which offer a remarkable example of a growing complex self-organizing adaptive system, exhibiting Zipf’s law over four full decades.},
  Issue                    = {21},
  Numpages                 = {4},
  Publisher                = {American Physical Society}
}

@Article{MalhotraMajchrzakRosen07,
  Title                    = {Leading Virtual Teams},
  Author                   = {Arvind Malhotra and Ann Majchrzak and Benson Rosen},
  Journal                  = {The Academy of Management Perspectives},
  Year                     = {2007},

  Month                    = feb,
  Number                   = {1},
  Pages                    = {60--69},
  Volume                   = {21},

  Abstract                 = {Virtual teams, whose members are geographically dispersed and cross-functional yet work on highly interdependent tasks, present unique leadership challenges. Based on our observations, interviews, and survey data, we identify six leadership practices of effective leaders of virtual teams. Specifically, we elaborate how leaders of successful virtual teams: 1) establish and maintain trust through the use of communication technology; 2) ensure that distributed diversity is understood and appreciated; 3) manage virtual work-life cycle (meetings); 4) monitor team progress using technology; 5) enhance visibility of virtual members within the team and outside in the organization; and 6) enable individual members of the virtual team to benefit from the team. These practices of virtual team leaders can be used to establish a foundation for training and developing future virtual team leaders.},
  Publisher                = {Academy of Management}
}

