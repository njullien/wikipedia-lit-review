@InProceedings{DenBestenetal10,
  Title                    = {Using metadata signals to support stigmergy},
  Author                   = {Den Besten,M. and Gaio,L. and Rossi,A. and Dalle,J. -.},
  Booktitle                = {Proceedings - 2010 4th IEEE International Conference on Self-Adaptive and Self-Organizing Systems Workshop, SASOW 2010},
  Year                     = {2010},
  Editor                   = { },
  Pages                    = {131-135},

  Abstract                 = {{Wikipedia, the "free encyclopedia that anyone can edit", has evolved substantially over the years according to the editors’ needs for more coordination and better quality management. In particular, metadata in the form of "templates" added to pages have come to play a critical role, working in a sense like stigmergic "stigmata" or "signals" which editors use to orient the efforts of their colleagues. We wonder whether such signals can be used to actually coordinate more effectively the collective efforts of editors. Based on an empirical analysis of two types of signals in Wikipedia, namely the "unsimple" and "NPOV" templates, we provide mixed evidence to support this claim. The effectiveness of stigmergic metadata signals appears to be affected by the division of labor within the Wikipedia community between core and more peripheral members.}}
}

