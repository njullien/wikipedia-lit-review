@Article{Rahman08,
  Title                    = {{An Analysis of Wikipedia}},
  Author                   = {Rahman, M.},
  Journal                  = {Journal of Information Technology Theory and Application},
  Year                     = {2008},
  Pages                    = {81},
  Volume                   = {9},

  Abstract                 = {One of the most popular Internet sites is Wikipedia. It is defined by its founders as the"free encyclopedia that anyone can edit." This property, we argue, makes Wikipedia a public good and hence subject to underprovision. A puzzling feature of Wikipedia however is its enormous size, at roughly seven times that of its commercial counterparts. What is driving this growth? And can we rely on this giant encyclopedia arising from free-editing? In this paper, we model contribution to Wikipedia and its reliability. We demonstrate that Wikipedia is indeed subject to free-riding, and offer a novel explanation for the mitigation of underprovision under such circumstances. We also find that free-riding introduces a lower-bound in the quality of Wikipedia. We identify Wikipedia as part of a general Internet phenomenon that we call the Collaborative Net, and that includes features such as citizen journalism. Our results contribute to the existing literature on public-goods, and also have important implications for the much debated topic of credibility in this new Collaborative Net environment characterized by phenomenal growth and information provided by volunteers.}
}

@Article{RajanWulf06,
