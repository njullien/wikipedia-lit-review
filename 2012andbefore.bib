@Article{Driskelletal06,
  Title                    = {What makes a good team player? Personality and team effectiveness.},
  Author                   = {Driskell, James E and Goodwin, Gerald F and Salas, Eduardo and O'Shea, Patrick Gavan},
  Journal                  = {Group Dynamics: Theory, Research, and Practice},
  Year                     = {2006},
  Number                   = {4},
