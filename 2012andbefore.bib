@Article{Stviliaetal09,
  Title                    = {{Issues of cross-contextual information quality evaluation--The case of Arabic, English, and Korean Wikipedias}},
  Author                   = {Stvilia, Besiki and Al-Faraj, A. and Yi, Y.},
  Journal                  = {Library \& Information Science Research},
  Year                     = {2009},
  Number                   = {4},
  Pages                    = {232-239},
  Volume                   = {31},

  Abstract                 = {An initial exploration into the issue of information quality evaluation across different cultural and community contexts based on data collected from the Arabic, English, and Korean Wikipedias showed that different Wikipedia communities may have different understandings of and models for quality. It also showed the feasibility of using some article edit-based metrics for automated quality measurement across different Wikipedia contexts. A model for measuring context similarity was developed and used to evaluate the relationship between similarities in sociocultural factors and the understanding of information quality by the three Wikipedia communities.}
}

@Article{StviliaGasser08,
  Title                    = {An activity theoretic model for information quality change},
  Author                   = {Stvilia, Besiki and Gasser, L.},
  Journal                  = {First Monday},
  Year                     = {2008},
  Number                   = {4},
  Volume                   = {13},

  Abstract                 = {To manage information quality (IQ) effectively, one needs to know how IQ changes over time, what causes it to change, and whether the changes can be predicted. In this paper we analyze the structure of IQ change in Wikipedia, an open, collaborative general encyclopedia. We found several patterns in Wikipedia’s IQ process trajectories and linked them to article types. Drawing on the results of our analysis, we develop a general model of IQ change that can be used for reasoning about IQ dynamics in many different settings, including traditional databases and information repositories.
},
  Url                      = {http://www.firstmonday.org/htbin/cgiwrap/bin/ojs/index.php/fm/article/view/2126/1951}
}

